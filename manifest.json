{
  "browser_action": {
    "default_popup": "web-modifier.html",
    "default_title": "Web Modifier 1.0"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "https://*.facebook.com/*",
        "https://*.youtube.com/*",
        "https://www.google.com/*",
        "https://www.google.de/*",
        "https://consent.google.com/*",
        "https://consent.youtube.com/*",
        "https://consent.youtube.de/*",
        "https://consent.google.de/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "description": "This extension can modify webpages",
  "manifest_version": 2,
  "name": "Web Modifier",
  "version": "1.0"
}